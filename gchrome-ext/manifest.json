{
  "manifest_version": 2,
  "name": "News Prettifier",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js": ["jquery-3.3.1.min.js","content.js"]
    }
  ],
  "permissions" : [
    "declarativeContent",
    "tabs"
  ],
  "page_action": {
    "default_icon": "statics/book.png",
    "default_popup": "popup.html"
  }
}
